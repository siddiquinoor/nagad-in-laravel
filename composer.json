{
    "name": "siddiquinoor/nagad-in-laravel",
    "description": "Nagad Payment Gateway for Laravel",
    "keywords": ["Nagad API", "Nagad Laravel"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "~6|~7|~8|~9|~10"
    },
    "autoload": {
        "psr-4": {
            "siddiquinoor\\NagadLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Siddiqui Noor",
            "email": "mr.noorbd@gmail.com"
        }
    ],   
    "extra": {
        "laravel": {
            "providers": [
                "NagadLaravel\\NagadServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
